{
  "name": "gatsby-theme-austere",
  "version": "0.0.2",
  "author": "John Otander",
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^2.15.28"
  },
  "dependencies": {
    "@emotion/core": "^10.0.17",
    "@mdx-js/react": "^1.5.0",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-theme-ui": "^0.2.43",
    "gatsby-theme-blog-core": "^1.1.0",
    "react-helmet": "^5.2.1",
    "theme-ui": "^0.2.43"
  },
  "devDependencies": {
    "husky": "^3.0.7",
    "lint-staged": "^9.4.1",
    "prettier": "^1.18.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,mdx,json}": [
      "prettier --write --no-semi --single-quote",
      "git add"
    ]
  }
}
